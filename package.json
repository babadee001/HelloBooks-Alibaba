{
  "name": "hello-books",
  "version": "1.0.0",
  "description": "Simple application that helps manage a library and its processes",
  "main": "server.js",
  "scripts": {
    "build": "webpack --progress --config ./webpack.config.prod.js",
    "e2e-setup": "selenium-standalone install",
    "e2e-test": "selenium-standalone start",
    "e2e:test": "./node_modules/.bin/nightwatch",
    "heroku-postbuild": "NODE_ENV=production && npm run build",
    "dev": "webpack-dev-server --inline --hot",
    "client:test": "jest --coverage && npm run coverage",
    "test:watch": "npm run client:test -- --watch",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "coverage": "./node_modules/.bin/nyc --reporter=html --reporter=text mocha --reporter spec --timeout 40000 --compilers js:babel-core/register test",
    "start": "nodemon --exec babel-node app.js",
    "str": "babel-node webpack-dev-server --config webpack.config.js --hot --port 3000",
    "lint": "./node_modules/.bin/eslint .",
    "test": "NODE_ENV=test && ./node_modules/.bin/mocha --timeout 40000 --compilers js:babel-core/register",
    "cover": "babel-node ./node_modules/istanbul/lib/cli cover node_modules/mocha/bin/_mocha  -- -R spec --timeout 40000",
    "db:seed": "DATABASE_URL=$DATABASE_URL sequelize db:seed --seed server/seeders/20180209124637-users.js"
  },
  "engines": {
    "node": "8.9.4"
  },
  "keywords": [
    "library"
  ],
  "author": "temidayo oyedele",
  "license": "ISC",
  "jest": {
    "verbose": true,
    "roots": [
      "./client"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "setupFiles": [
      "<rootDir>/test-env.js"
    ],
    "setupTestFrameworkScriptFile": "./client/__tests__/setup.js",
    "testPathIgnorePatterns": [
      "client/__tests__/__mocks__",
      "client/__tests__/setup.js"
    ],
    "moduleNameMapper": {
      "\\.scss$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ]
  },
  "dependencies": {
    "axios": "^0.12.0",
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.17.2",
    "chai": "^4.1.1",
    "chai-http": "^3.0.0",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.17",
    "coveralls": "^2.13.1",
    "dotenv": "^4.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "eslint": "^4.3.0",
    "expect": "^22.1.0",
    "express": "^4.15.3",
    "express-validator": "^3.2.1",
    "faker": "^4.1.0",
    "firebase": "^4.6.2",
    "firebase-mock": "^1.1.13",
    "hammerjs": "^2.0.8",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.1.4",
    "jquery": "^2.2.1",
    "jsonwebtoken": "^7.4.2",
    "load-js": "^2.0.0",
    "lodash": "^4.17.4",
    "materialize-css": "^0.100.2",
    "mocha": "^3.5.0",
    "morgan": "^1.8.2",
    "moxios": "^0.4.0",
    "nodemon": "^1.11.0",
    "node-sass": "^4.11.0",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-firebase-image-uploader": "^1.1.0",
    "react-google-login": "^3.0.2",
    "react-hot-loader": "^1.3.1",
    "react-materialize": "^1.0.14",
    "react-redux": "^5.0.6",
    "react-router": "^3.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-mock-store": "^1.5.0",
    "redux-thunk": "^2.2.0",
    "sequelize": "^4.22.11",
    "sinon": "^4.2.2",
    "supertest": "^3.0.0",
    "swagger-jsdoc": "^1.3.0",
    "sweetalert": "^2.0.8",
    "validator": "^9.0.0",
    "webpack": "^3.5.5",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-merge": "^4.1.1",
    "whatwg-fetch": "^2.0.3"
  },
